<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>PIMap_MainLogo.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="PIMap_UserControls\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="PIMap_Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>PIMap_Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="PIMap_Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>PIMap_Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>